[Desktop Entry]
Type=Application
Version=1.0
Name=Jupyter Notebook
# This is for opening files, not launching the notebook from a menu
NoDisplay=true
Exec={PYTHON} -m nbopen %f
Terminal=false
MimeType=application/x-ipynb+json;
Icon=jupyter

[Desktop Entry]
Type=Application
Version=1.0
Name=Jupyter Notebook launcher
Exec={PYTHON} -m nbopen %u
Terminal=true
MimeType=x-scheme-handler/anaconda-org-notebook;
